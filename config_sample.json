{
  "--proxy": {
    "type": "text",
    "description": "是否启用代理",
    "placeholder": "socks5://127.0.0.1:1080",
    "value": "socks5://192.168.199.1:18090"
  },
  "--windows-filenames": {
    "type": "checkbox",
    "description": "强制 Windows 格式文件名",
    "checked": true
  },
  "--playlist-reverse": {
    "type": "checkbox",
    "description": "反转播放列表编号顺序",
    "checked": true
  },
  "--write-description": {
    "type": "checkbox",
    "description": "保存视频描述",
    "checked": false
  },
  "--write-info-json": {
    "type": "checkbox",
    "description": "保存视频信息",
    "checked": false
  },
  "--write-playlist-metafiles": {
    "type": "checkbox",
    "description": "保存播放列表元数据",
    "checked": false
  },
  "--format": {
    "type": "text",
    "placeholder": "bv*+ba/b",
    "description": "保存质量",
    "value": "bv*+ba/b"
  },
  "--merge-output-format": {
    "type": "text",
    "placeholder": "mp4",
    "description": "合并视频格式",
    "value": "mp4"
  },
  "--sub-format": {
    "type": "text",
    "placeholder": "ass/best/srt",
    "description": "字幕格式",
    "checked": true,
    "value": "best"
  },
  "--sub-langs": {
    "type": "text",
    "placeholder": "zh*,chin,en.*",
    "description": "字幕语言",
    "checked": true,
    "value": "zh*,chin*,en.*"
  },
  "--write-subs": {
    "type": "checkbox",
    "description": "保存字幕",
    "checked": false
  },
  "--embed-subs": {
    "type": "checkbox",
    "description": "合并字幕",
    "checked": false
  },
  "--write-thumbnail": {
    "type": "checkbox",
    "description": "保存缩略图",
    "checked": false
  },
  "--embed-thumbnail": {
    "type": "checkbox",
    "description": "合并缩略图",
    "checked": false
  },
  "--embed-metadata": {
    "type": "checkbox",
    "description": "合并元数据",
    "checked": false
  },
  "--embed-chapters": {
    "type": "checkbox",
    "description": "合并章节信息",
    "checked": false
  },
  "--paths": {
    "type": "text",
    "placeholder": "d:\\Downloads\\",
    "required": true,
    "description": "保存根目录",
    "value": "d:\\H\\Downloads\\"
  },
  "--cookies-from-browser": {
    "type": "text",
    "placeholder": "firefox:d:\\firefox_profile\\",
    "description": "使用浏览器Cookie",
    "value": "firefox:d:\\H\\Documents\\firefox_profile"
  }
}